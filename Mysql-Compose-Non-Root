

version: '3'
services:
  db:
    build:
      context: "."
      dockerfile: ./Dockerfile
    environment:
      MYSQL_ROOT_PASSWORD: 1234
    ports:
      - '3306:3306'
    healthcheck:
      test: ["CMD-SHELL", 'mysqladmin ping']
      interval: 10s
      timeout: 5s
      retries: 10
      
      
      
FROM mysql:8
USER: mysql 

